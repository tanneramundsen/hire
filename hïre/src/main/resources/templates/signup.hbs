    <div class="page-wrapper bg-gra-02 p-t-130 p-b-100 font-poppins" style="background-image: url('assets/images/slide-01.jpg');">
        <div class="wrapper wrapper--w680">
            <div class="card card-4">
                <div class="card-body">
                    <h2 class="title">Sign Up Form</h2>
                    <form action="/signup" method="post">
                        <div class="row row-space">
                            <div class="col-md-6 col-sm-12">
                                <div class="input-group" data-validate = "Name is required">
                                    <label class="label">First Name</label>
                                    <input class="input--style-4" type="text" name="firstName">
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-12" data-validate = "Name is required">
                                <div class="input-group">
                                    <label class="label">Last Name</label>
                                    <input class="input--style-4" type="text" name="lastName">
                                </div>
                            </div>
                        </div>
                        <div class="row row-space">
                            <div class="col-md-6 col-sm-12">
                                <div class="input-group" data-validate = "JHED is required">
                                    <label class="label">JHED</label>
                                    <input class="input--style-4" type="text" name="jhed">
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-12">
                                <div class="input-group">
                                    <label class="label">Choose account type:</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="profileType">
                                            <option disabled="disabled" selected="selected">I am a..</option>
                                            <option>Student</option>
                                            <option>Professor</option>
                                            <option>Admin</option>
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row row-space">
                            <div class="col-md-6 col-sm-12">
                                <div class="input-group">
                                    <label class="label">Choose courses:</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="courses" multiple="multiple">
                                            <option disabled="disabled" selected="selected">select courses</option>
                                            {{#each allCourses}}
                                                <option>{{name}}</option>
                                            {{/each}}
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="p-t-15">
                            <button class="btn btn--radius-2 btn--blue" type="submit">Submit</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
