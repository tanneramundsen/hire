<div class="page-wrapper bg-gra-02 p-t-130 p-b-100 font-poppins" style="background-image: url('../../../assets/images/slide-01.jpg');">
    <div class="wrapper wrapper--w680">
        <div class="card card-4">
            <div class="card-body">
                <h2> {{name}}</h2>
                <form action="/{{id}}/courseinfo/{{jhed}}/studentview" method="post">
                    {{#if shortlistStatus}}
                        <input name="shortlistStatus" type="checkbox" checked data-toggle="toggle" data-on="Shortlisted" data-off="Shortlist" data-onstyle="primary" data-offstyle="secondary">
                        {{else}}
                        <input name="shortlistStatus" type="checkbox" data-toggle="toggle" data-on="Shortlisted" data-off="Shortlist" data-onstyle="primary" data-offstyle="secondary">
                    {{/if}}
                    <div class="row">
                        <button type="submit" id="form-submit" class="main-button">Update<i class="fa fa-arrow-right"></i></button>
                    </div>
                    <h3> General Information: </h3>
                    <p> Jhed: {{jhed}}</p>
                    <p> Email: {{email}}</p>
                    <p> Year: {{year}}</p>
                    <p> GPA: {{gpa}}</p>
                    <p> Number of Credits for Spring 2020: {{credits}}</p>
                    <p> Majors/Minors: {{majorAndMinor}}</p>
                    <p> Reference: {{reference}}</p>
                    <p> Resume: {{resume}}</p>
                    {{#if isAdmin}}
                        <h3> Admin-only information: </h3>
                        <p> FWS: {{fws}}</p>
                        <p> Student Status: {{studentStatus}} </p>
                        <p> Most Recent Payroll: {{mostRecentPayroll}} </p>
                        <p> Other jobs: {{otherJobs}} </p>
                        <p> Hours Available: {{hoursAvailable}}</p>
                    {{/if}}
                    <br>
                    <div class="row">
                        <div class="col-md-8 col-sm-12">
                            <h3> Student's Preferred classes: </h3>
                            <div class="list-inline">
                                {{#if courseOne}}
                                    <p> 1. {{courseOne}}</p>
                                {{else}}
                                    <p> 1. Not ranked yet! </p>
                                {{/if}}
                                {{#if courseTwo}}
                                    <p> 2. {{courseTwo}}</p>
                                {{else}}
                                    <p> 2. Not ranked yet! </p>
                                {{/if}}
                                {{#if courseThree}}
                                    <p> 3. {{courseThree}}</p>
                                {{else}}
                                    <p> 3. Not ranked yet! </p>
                                {{/if}}
                            </div>
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-md-8 col-sm-12">
                            <h3> Course Specific Information </h3>
                            <div class="list-inline">
                                {{#each courseSpecificInfo}}
                                    <h4> {{@key}} </h4>
                                    {{#each this}}
                                        <p>{{@key}} : {{this}}</p>
                                    {{/each}}
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
